{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "GQL Docs",
  "colors": {
    "primary": "#BD93F9",
    "light": "#BD93F9",
    "dark": "#BD93F9"
  },
  "favicon": "/images/gql_ch_favicon.png",
  "seo": {
    "metatags": {
        "canonical": "https://gql.ch"
    }
  }, 
  "navigation": {
    "tabs": [
      {
        "tab": "Docs",
        "groups": [
          {
            "group": "Querying",
            "pages": [
              "docs/querying/where",
              "docs/querying/match"
            ]
          },
          {
            "group": "Functions",
            "pages": [
              "docs/functions/reduce"
            ]
          }
        ]
      }
    ]
  },
  "logo": "/images/gql_ch_icon.svg",
  "navbar": {
    "links": [],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://dashboard.mintlify.com"
    }
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
  },
  "footer": {
    "socials": {
      "website": "https://gql.net"
    }
  },
  "appearance": {
    "default": "dark",
    "strict": true
  },
  "background": {
    "color": {
      "dark": "#282A36"
    }
  }
}